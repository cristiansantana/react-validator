{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "jsx": "react-jsx",
    "lib": ["ES5", "ES6", "dom"],
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
